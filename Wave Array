void convertToWave(int *arr, int n){
    
    // Your code here
     
    // Your code here
    //sort(arr,arr+n);
    bool f=true;
    for(int i=0;i<n-1;i++){
        if(f){
            if(arr[i]<arr[i+1]){
            swap(arr[i],arr[i+1]);
        }
        }else if(arr[i+1]<arr[i]){
            swap(arr[i],arr[i+1]);
        }
        f=!f;
        
        
    }
    
}
